{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{js,ts,tsx,json}'",
    "typecheck": "tsc --noEmit",
    "pod-install": "cd ios && bundle exec pod install ; cd ..",
    "android:bundle": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output './android/app/src/main/assets/index.android.bundle' --assets-dest './android/app/src/main/res'",
    "fastlane": "bundle exec fastlane",
    "build:android_debug_apk": "yarn fastlane android debug_apk",
    "build:android_release_apk": "yarn fastlane android release_apk",
    "build:android_release_aab": "yarn fastlane android release_aab",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@tanstack/react-query": "^5.51.15",
    "axios": "^1.6.2",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-keychain": "8.1.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-vector-icons": "^10.0.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.7",
    "reactotron-react-native-mmkv": "^0.2.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "resolutions": {
    "reactotron-core-client@2.9.3": "patch:reactotron-core-client@npm%3A2.9.3#./.yarn/patches/reactotron-core-client-npm-2.9.3-3669949e7d.patch"
  }
}
